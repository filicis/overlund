<!DOCTYPE html>
<html lang={{ app.request.getLocale() }}>

	{# Document Header #}

  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="application-name" content="Codename Overlund">
    <meta name="description" content="Overlund Gedcom Editor">
    <meta name="generator" content="Symfony 6.1">
    <meta name="keywords" content="Gedcom Editor">

  	<meta name="author" content="Michael Lindhardt Rasmussen">

    <title>{% block title %}{{ product }}{% endblock %}</title>

    <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22>
      <text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">


    {% block stylesheets %}

      {# Vi anvender Bootstrap - aktuelt version 5.2.2 #}


      <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">

      {# Vi anvender Bootstrap Icons - aktuelt version 1.9.1 #}

      <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.9.1/font/bootstrap-icons.css">

      {# Encore #}

      {{ encore_entry_link_tags('app') }}

    {% endblock stylesheets %}




    {% block javascripts %}

      {# { encore_entry_script_tags('app') } #}

    {% endblock %}
  </head>


	{# Document Body #}

  <body>

		{% block navbar %}

   		{{ include('navbar-primary.html.twig') }}

	  {% endblock navbar %}


    {% block body %}
    {% endblock body %}


    {% block jjavascripts %}

      {{ encore_entry_script_tags('app') }}


      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>

			<script>

				var tooltipTriggerList = [].slice.call(document.querySelectorAll('[data-bs-toggle="tooltip"]'))
				var tooltipList = tooltipTriggerList.map(function (tooltipTriggerEl) {
  				return new bootstrap.Tooltip(tooltipTriggerEl)
					})


			</script>

    {% endblock jjavascripts %}

  </body>
</html>
